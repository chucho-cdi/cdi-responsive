			 $(function() {
			   $('a[rel=izquierda]').tipsy({fade: true, gravity: 'ne'});
			   $('a[rel=centro]').tipsy({fade: true, gravity: 'n'});
			   $('a[rel=derecha]').tipsy({fade: true, gravity: 'nw'});
