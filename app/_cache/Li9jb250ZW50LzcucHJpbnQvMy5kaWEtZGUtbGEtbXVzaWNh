			 $(function() {
			   $('a[rel=izquierda]').tipsy({fade: true, gravity: 'ne', html: true});
			   $('a[rel=centro]').tipsy({fade: true, gravity: 'n'});
			   $('a[rel=derecha]').tipsy({fade: true, gravity: 'nw', html: true});
			 });
        </script>
            
        
        <script>
		<!--http://jqueryfordesigners.com/fixed-floating-elements/ -->
			$(function () {
			  
			  var msie6 = $.browser == 'msie' && $.browser.version < 7;
			  
			  if (!msie6) {
				var top = $('#titulo').offset().top - parseFloat($('#titulo').css('margin-top').replace(/auto/, 0));
				$(window).scroll(function (event) {
				  // what the y position of the scroll is
				  var y = $(this).scrollTop();
				  // whether that's below the form
				  if (y >= top) {
					// if so, ad the fixed class			
					$('#titulo').addClass('fixed');						 
					} else {
					// otherwise remove it
					$('#titulo').removeClass('fixed');
				  }
					});
			  }  
			});
		</script>

                
